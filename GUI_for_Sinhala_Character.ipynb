   "metadata": {},
   "outputs": [],
   "source": [
    "width=600\n",
    "height=600\n",
    "\n",
    "win = tk.Tk()\n",
    "\n",
    "## create canvas where to write\n",
    "canvas = tk.Canvas(win, width= width, height=height, bg =\"white\")\n",
    "canvas.grid(row=0, column=0)\n",
    "\n",
    "win.mainloop()"
